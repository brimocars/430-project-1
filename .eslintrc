// Borrowed from HTTP-API-Assignment-ii
{
  "extends": "airbnb/base",
  "rules": {
    "no-underscore-dangle": "off",
    "no-plusplus": "off",
    "import/extensions": "off",
    "max-len": ["error", { "code": 120 }],
    "no-console": "off",
    "no-param-reassign": "off"
  },
  "parserOptions": {
    "ecmaVersion": 2020
  }
}